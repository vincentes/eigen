{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "python311",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "__pycache__",
        "*.pyc",
        ".pytest_cache"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5001,
      "timeout": "540s"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "storage": {
      "port": 9199
    },
    "pubsub": {
      "port": 8085
    }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
